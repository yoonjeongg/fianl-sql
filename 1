CREATE TABLE "MEMBER" (
	"NO"	NUMBER		NOT NULL,
	"ID"	VARCHAR2(50)		NOT NULL,
	"PWD"	VARCHAR2(50)		NOT NULL,
	"DEP_NO"	NUMBER		NULL,
	"POSITION_NO"	NUMBER		NOT NULL,
	"GROUP_NO"	NUMBER		NULL,
	"NAME"	VARCHAR2(20)		NOT NULL,
	"GENDER"	VARCHAR2(2)		NULL,
	"EMAIL"	VARCHAR2(50)		NULL,
	"PHONE"	NUMBER		NULL,
	"STATUS"	VARCHAR2(2)	DEFAULT 'Y'	NOT NULL
);

COMMENT ON COLUMN "MEMBER"."NO" IS '사원 번호';

COMMENT ON COLUMN "MEMBER"."ID" IS '아이디';

COMMENT ON COLUMN "MEMBER"."PWD" IS '비밀번호';

COMMENT ON COLUMN "MEMBER"."DEP_NO" IS '부서번호';

COMMENT ON COLUMN "MEMBER"."POSITION_NO" IS '직책아이디';

COMMENT ON COLUMN "MEMBER"."GROUP_NO" IS '근무그룹번호';

COMMENT ON COLUMN "MEMBER"."NAME" IS '이름';

COMMENT ON COLUMN "MEMBER"."GENDER" IS '성별';

COMMENT ON COLUMN "MEMBER"."EMAIL" IS '이메일';

COMMENT ON COLUMN "MEMBER"."PHONE" IS '연락처';

COMMENT ON COLUMN "MEMBER"."STATUS" IS '재직상태';

CREATE TABLE "RES_MEETING" (
	"RES_NO"	NUMBER		NOT NULL,
	"NO"	NUMBER		NOT NULL,
	"ROOM_NO"	NUMBER		NOT NULL,
	"START_TIME"	DATE		NOT NULL,
	"END_TIME"	DATE		NOT NULL,
	"MEET_PURPOSE"	CLOB		NULL,
	"DEL_YN"	VARCHAR2(2)	DEFAULT 'N'	NOT NULL
);

COMMENT ON COLUMN "RES_MEETING"."RES_NO" IS '예약번호';

COMMENT ON COLUMN "RES_MEETING"."NO" IS '사원번호';

COMMENT ON COLUMN "RES_MEETING"."ROOM_NO" IS '룸번호';

COMMENT ON COLUMN "RES_MEETING"."START_TIME" IS '예약시작시간';

COMMENT ON COLUMN "RES_MEETING"."END_TIME" IS '예약종료시간';

COMMENT ON COLUMN "RES_MEETING"."MEET_PURPOSE" IS '예약목적';

COMMENT ON COLUMN "RES_MEETING"."DEL_YN" IS '예약삭제여부';

CREATE TABLE "POST" (
	"POST_NO"	NUMBER		NOT NULL,
	"POST_TITLE"	VARCHAR2(100)		NOT NULL,
	"POST_CONTENT"	VARCHAR2(3000)		NOT NULL,
	"POST_DATE"	DATE	DEFAULT SYSDATE	NOT NULL,
	"POST_UPDATE"	DATE	DEFAULT SYSDATE	NULL,
	"NOTICE_END_DATE"	DATE		NULL,
	"POST_STATUS"	VARCHAR2(50)	DEFAULT 'Y'	NOT NULL,
	"NO"	NUMBER		NOT NULL,
	"CATEGORY_NO"	NUMBER		NOT NULL
);

COMMENT ON COLUMN "POST"."POST_NO" IS '게시글 아이디 (SEQ_PONO)';

COMMENT ON COLUMN "POST"."POST_TITLE" IS '게시글 제목';

COMMENT ON COLUMN "POST"."POST_CONTENT" IS '게시글 내용';

COMMENT ON COLUMN "POST"."POST_DATE" IS '게시글 작성날짜';

COMMENT ON COLUMN "POST"."POST_UPDATE" IS '게시글 수정날짜';

COMMENT ON COLUMN "POST"."NOTICE_END_DATE" IS '공지노출 마감날짜';

COMMENT ON COLUMN "POST"."POST_STATUS" IS 'Y 기본값 N 삭제';

COMMENT ON COLUMN "POST"."NO" IS '작성자 사원번호';

COMMENT ON COLUMN "POST"."CATEGORY_NO" IS '카테고리 아이디';

CREATE TABLE "AUTHORITY" (
	"AUTH_NO"	NUMBER		NOT NULL,
	"AUTH_NAME"	VARCHAR2(20)		NOT NULL
);


COMMENT ON COLUMN "AUTHORITY"."AUTH_NO" IS '권한번호';

COMMENT ON COLUMN "AUTHORITY"."AUTH_NAME" IS '권한명';

CREATE TABLE "RES_CAR" (
	"RES_NO"	NUMBER		NOT NULL,
	"NO"	NUMBER		NOT NULL,
	"CAR_NO"	NUMBER		NOT NULL,
	"START_TIME"	DATE		NOT NULL,
	"END_TIME"	DATE		NOT NULL,
	"CAR_PURPOSE"	CLOB		NULL,
	"DEL_YN"	VARCHAR2(2)	DEFAULT 'N'	NOT NULL
);

COMMENT ON COLUMN "RES_CAR"."RES_NO" IS '예약번호';

COMMENT ON COLUMN "RES_CAR"."NO" IS '사원번호';

COMMENT ON COLUMN "RES_CAR"."CAR_NO" IS '차량식별번호';

COMMENT ON COLUMN "RES_CAR"."START_TIME" IS '예약시작시간';

COMMENT ON COLUMN "RES_CAR"."END_TIME" IS '예약종료시간';

COMMENT ON COLUMN "RES_CAR"."CAR_PURPOSE" IS '예약목적';

COMMENT ON COLUMN "RES_CAR"."DEL_YN" IS '예약삭제여부';

CREATE TABLE "COM_CAR" (
	"CAR_NO"	NUMBER		NOT NULL,
	"CAR_NAME"	VARCHAR2(30)		NOT NULL,
	"CAR_NUMBER"	NUMBER		NOT NULL,
	"CAR_YN"	VARCHAR2(5)	DEFAULT 'Y'	NOT NULL,
	"DEL_YN"	VARCHAR2(2)	DEFAULT 'N'	NULL
);

COMMENT ON COLUMN "COM_CAR"."CAR_NO" IS '차량식별번호';

COMMENT ON COLUMN "COM_CAR"."CAR_NAME" IS '차량명';

COMMENT ON COLUMN "COM_CAR"."CAR_NUMBER" IS '차량번호';

COMMENT ON COLUMN "COM_CAR"."CAR_YN" IS '사용가능여부';

COMMENT ON COLUMN "COM_CAR"."DEL_YN" IS '삭제여부';

CREATE TABLE "APPROVAL" (
	"APP_NO"	NUMBER		NOT NULL,
	"APP_MEM_NO"	NUMBER		NOT NULL,
	"APP_TITLE"	VARCHAR2(1000)		NOT NULL,
	"APP_CONTENT"	VARCHAR2(3000)		NOT NULL,
	"APP_STATUS"	VARCHAR2(10)	DEFAULT '대기'	NOT NULL,
	"APP_CREATED_DATE"	DATE	DEFAULT SYSDATE	NOT NULL,
	"APP_DUE_DATE"	DATE		NOT NULL
);

COMMENT ON COLUMN "APPROVAL"."APP_NO" IS 'SEQ_APPNO';

COMMENT ON COLUMN "APPROVAL"."APP_MEM_NO" IS '작성자 사원번호';

COMMENT ON COLUMN "APPROVAL"."APP_TITLE" IS '문서 제목';

COMMENT ON COLUMN "APPROVAL"."APP_CONTENT" IS '문서 내용';

COMMENT ON COLUMN "APPROVAL"."APP_STATUS" IS '임시/대기/진행/승인/반려/회수';

CREATE TABLE "IMG" (
	"IMG_NO"	NUMBER		NOT NULL,
	"ORG_NAME"	VARCHAR2(100)		NOT NULL,
	"SVR_NAME"	VARCHAR2(100)		NOT NULL,
	"IMG_PATH"	VARCHAR2(1000)		NOT NULL,
	"IMG_TYPE"	VARCHAR2(100)		NULL
);

COMMENT ON COLUMN "IMG"."IMG_NO" IS '사진 아이디 (SEQ_IMGNO)';

COMMENT ON COLUMN "IMG"."ORG_NAME" IS '원본 파일이름';

COMMENT ON COLUMN "IMG"."SVR_NAME" IS '서버저장 파일이름';

COMMENT ON COLUMN "IMG"."IMG_PATH" IS '사진파일 저장경로';

COMMENT ON COLUMN "IMG"."IMG_TYPE" IS '사진파일 타입';

CREATE TABLE "POST_CATEGORY" (
	"CATEGORY_NO"	NUMBER		NOT NULL,
	"CATEGORY_NAME"	VARCHAR2(30)		NOT NULL
);


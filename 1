CREATE TABLE "MESSAGE" (
	"MSG_CODE"	NUMBER		NOT NULL,
	"SENDER_NO"	NUMBER		NOT NULL,
	"REICIPIENT_NO"	NUBMER		NOT NULL,
	"MSG_CONTENT"	VARCHAR2(2000)		NOT NULL,
	"SEND_DATE"	CHAR(8)		NOT NULL,
	"SEND_TIME"	CHAR(6)		NOT NULL,
	"READ_DATE"	CHAR(6)		NULL,
	"READ_TIME"	CHAR(6)		NULL,
	"RECYCLE_BIN_STATUS"	CHAR(1)	DEFAULT 'N'	NOT NULL,
	"DELETE_STATUS"	CHAR(1)	DEFAULT 'N'	NOT NULL
);

COMMENT ON COLUMN "MESSAGE"."MSG_CODE" IS '쪽지번호';

COMMENT ON COLUMN "MESSAGE"."SENDER_NO" IS '쪽지발신자';

COMMENT ON COLUMN "MESSAGE"."REICIPIENT_NO" IS '쪽지수신자';

COMMENT ON COLUMN "MESSAGE"."MSG_CONTENT" IS '쪽지내용(1000자제한)';

COMMENT ON COLUMN "MESSAGE"."SEND_DATE" IS '발신날짜';

COMMENT ON COLUMN "MESSAGE"."SEND_TIME" IS '발신시간';

COMMENT ON COLUMN "MESSAGE"."READ_DATE" IS '읽은날짜';

COMMENT ON COLUMN "MESSAGE"."READ_TIME" IS '읽은시간';

COMMENT ON COLUMN "MESSAGE"."RECYCLE_BIN_STATUS" IS 'N기본값 Y 휴지통처리';

COMMENT ON COLUMN "MESSAGE"."DELETE_STATUS" IS 'N기본값 Y삭제처리';


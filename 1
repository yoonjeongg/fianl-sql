	"ATTACH_NO"	NUMBER		NULL,
	"ORG_NAME"	VARCHAR2(500)		NULL,
	"SAVED_NAME"	VARCHAR2(500)		NULL,
	"SAVE_PATH"	VARCHAR2(1000)		NULL
);


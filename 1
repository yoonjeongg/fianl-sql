CREATE TABLE "WORK_CATEGORY" (
	"WORK_NO"	NUMBER		NOT NULL,
	"WORK_NAME"	VARCHAR2(50)		NOT NULL,
	"WORK_INCLUDE"	VARCHAR2(50)		NULL,
	"WORK_USE"	VARCHAR2(50)		NULL,
	"WORK_STATUS"	VARCHAR2(2)	DEFAULT 'Y'	NOT NULL,
	"WORK_DATE"	DATE	DEFAULT SYSDATE	NULL,
	"WORK_MODDATE"	DATE	DEFAULT SYSDATE	NULL
);

COMMENT ON COLUMN "WORK_CATEGORY"."WORK_NO" IS '근무유형 카테고리번호';

COMMENT ON COLUMN "WORK_CATEGORY"."WORK_NAME" IS '근무유형 카테고리이름';

COMMENT ON COLUMN "WORK_CATEGORY"."WORK_INCLUDE" IS '근무시간포함';

COMMENT ON COLUMN "WORK_CATEGORY"."WORK_USE" IS '사용여부';

COMMENT ON COLUMN "WORK_CATEGORY"."WORK_STATUS" IS '상태';

COMMENT ON COLUMN "WORK_CATEGORY"."WORK_DATE" IS '작성일';

COMMENT ON COLUMN "WORK_CATEGORY"."WORK_MODDATE" IS '수정일';

CREATE TABLE "OVERTIME" (
	"OVER_NO"	NUMBER		NOT NULL,
	"NO"	NUMBER		NOT NULL,
	"REP_NO"	NUMBER		NOT NULL,
	"OVER_DATE"	DATE	DEFAULT SYSDATE	NULL,
	"OVER_MODDATE"	DATE	DEFAULT SYSDATE	NULL,
	"OVER_FIRWORK"	DATE		NULL,
	"OVER_WORKING"	DATE		NULL,
	"OVER_REASON"	VARCHAR2(2000)		NULL,
	"OVER_ENDWORK"	DATE		NULL,
	"OVER_STATUS"	VARCHAR2(2)	DEFAULT 'Y'	NOT NULL,
	"OVER_THREE"	VARCHAR2(50)		NULL,
	"OVER_OK"	VARCHAR2(50)	DEFAULT '미승인'	NOT NULL,
	"OVER_POSS"	DATE	DEFAULT SYSDATE	NULL
);

COMMENT ON COLUMN "OVERTIME"."OVER_NO" IS '게시글 번호';

COMMENT ON COLUMN "OVERTIME"."NO" IS '사원번호';


CREATE TABLE "ATT" (
	"ATT_NO"	NUMBER		NOT NULL,
	"NO"	NUMBER		NOT NULL,
	"WORK_NO"	NUMBER		NOT NULL,
	"ATT_GO"	DATE		NULL,
	"ATT_OFF"	DATE		NULL,
	"ATT_ADD"	DATE		NULL,
	"ATT_OVER"	DATE		NULL,
	"ATT_NIGHT"	DATE		NULL,
	"ATT_REASON"	VARCHAR2(2000)		NULL,
	"ATT_DATE"	DATE	DEFAULT SYSDATE	NULL,
	"ATT_STATUS"	VARCHAR2(2)	DEFAULT 'Y'	NOT NULL
);

COMMENT ON COLUMN "ATT"."ATT_NO" IS '근태관리번호';

COMMENT ON COLUMN "ATT"."NO" IS '사원 번호';

COMMENT ON COLUMN "ATT"."WORK_NO" IS '근무유형 카테고리번호';

COMMENT ON COLUMN "ATT"."ATT_GO" IS '출근시간';

COMMENT ON COLUMN "ATT"."ATT_OFF" IS '퇴근시간';

COMMENT ON COLUMN "ATT"."ATT_ADD" IS '총근무시간';

COMMENT ON COLUMN "ATT"."ATT_OVER" IS '연장근무시간';

COMMENT ON COLUMN "ATT"."ATT_NIGHT" IS '야간근무시간';

COMMENT ON COLUMN "ATT"."ATT_REASON" IS '근태수정이유';

COMMENT ON COLUMN "ATT"."ATT_DATE" IS '작성일';

COMMENT ON COLUMN "ATT"."ATT_STATUS" IS '상태';

CREATE TABLE "REPORT" (
	"REP_NO"	NUMBER		NOT NULL,
	"NO"	NUMBER		NOT NULL,

COMMENT ON COLUMN "POST_CATEGORY"."CATEGORY_NAME" IS '카테고리명';

CREATE TABLE "REPLY" (
	"REPLY_NO"	NUMBER		NOT NULL,
	"REPLY_CONTENT"	VARCHAR2(500)		NOT NULL,
	"REPLY_DATE"	DATE	DEFAULT SYSDATE	NOT NULL,
	"REPLY_UPDATE"	DATE	DEFAULT SYSDATE	NULL,
	"REPLY_STATUS"	VARCHAR2(2)	DEFAULT 'Y'	NOT NULL,
	"POST_NO"	NUMBER		NOT NULL,
	"NO"	NUMBER		NOT NULL,
	"REF_REPLY_NO"	NUMBER		NOT NULL
);

COMMENT ON COLUMN "REPLY"."REPLY_NO" IS '댓글 아이디 (REQ_RENO)';

COMMENT ON COLUMN "REPLY"."REPLY_CONTENT" IS '댓글 내용';

COMMENT ON COLUMN "REPLY"."REPLY_DATE" IS '작성날짜';

COMMENT ON COLUMN "REPLY"."REPLY_UPDATE" IS '댓글 수정날짜';

COMMENT ON COLUMN "REPLY"."REPLY_STATUS" IS 'Y 기본값 N 삭제';

COMMENT ON COLUMN "REPLY"."POST_NO" IS '게시글 아이디';

COMMENT ON COLUMN "REPLY"."NO" IS '작성자사원번호';

COMMENT ON COLUMN "REPLY"."REF_REPLY_NO" IS '참조 댓글 아이디';

CREATE TABLE "SURVEY" (
	"SURV_NO"	NUMBER		NOT NULL,
	"SURV_TITLE"	VARCHAR2(100)		NOT NULL,
	"SURV_DATE"	DATE	DEFAULT SYSDATE	NOT NULL,
	"SURV_END_DATE"	DATE		NOT NULL,
	"SURV_CONTENT"	VARCHAR2(1000)		NULL,
	"SURV_SUM"	NUMBER		NOT NULL,
	"SURV_NUM"	NUMBER	DEFAULT 0	NOT NULL,
	"NO"	NUMBER		NOT NULL,
	"DEP_NO"	NUMBER		NULL
);

COMMENT ON COLUMN "SURVEY"."SURV_NO" IS '설문 아이디 (SEQ_SVNO)';

COMMENT ON COLUMN "SURVEY"."SURV_TITLE" IS '설문 제목';

COMMENT ON COLUMN "SURVEY"."SURV_DATE" IS '설문 작성날짜';

COMMENT ON COLUMN "SURVEY"."SURV_END_DATE" IS '설문 종료일';

COMMENT ON COLUMN "SURVEY"."SURV_CONTENT" IS '설문 설명';

COMMENT ON COLUMN "SURVEY"."SURV_SUM" IS '설문 대상인원';

COMMENT ON COLUMN "SURVEY"."SURV_NUM" IS '설문 참가인원';

COMMENT ON COLUMN "SURVEY"."NO" IS '작성자사원번호';

COMMENT ON COLUMN "SURVEY"."DEP_NO" IS '설문 대상 부서';


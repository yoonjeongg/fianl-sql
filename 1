CREATE TABLE "CALENDAR" (
	"CAL_CODE"	NUMBER		NOT NULL,
	"CAL_CATEGORY_CODE"	VARCHAR2(10)		NOT NULL,
	"DEP_NO"	NUMBER		NOT NULL,
	"WRITER_NO"	NUMBER		NOT NULL,
	"CAL_NAME"	VARCHAR(100)		NOT NULL,
	"CAL_CONTENT"	VARCHAR2(2000)		NULL,
	"START_DATE"	CHAR(8)		NOT NULL,
	"END_DATE"	CHAR(8)		NOT NULL,
	"START_TIME"	CHAR(6)		NULL,
	"END_TIME"	CHAR(6)		NULL,
	"ALLDAY_STATUS"	CHAR(1)	DEFAULT 'N'	NOT NULL
);

COMMENT ON COLUMN "CALENDAR"."CAL_CODE" IS '일정번호';

COMMENT ON COLUMN "CALENDAR"."CAL_CATEGORY_CODE" IS '카테고리 아이디';

COMMENT ON COLUMN "CALENDAR"."DEP_NO" IS '부서번호';

COMMENT ON COLUMN "CALENDAR"."WRITER_NO" IS '작성자 아아디';

COMMENT ON COLUMN "CALENDAR"."CAL_NAME" IS '일정이름';

COMMENT ON COLUMN "CALENDAR"."CAL_CONTENT" IS '일정내용';

COMMENT ON COLUMN "CALENDAR"."START_DATE" IS '시작날짜';

COMMENT ON COLUMN "CALENDAR"."END_DATE" IS '종료날짜';

COMMENT ON COLUMN "CALENDAR"."START_TIME" IS '시작시간';

COMMENT ON COLUMN "CALENDAR"."END_TIME" IS '종료시간';

COMMENT ON COLUMN "CALENDAR"."ALLDAY_STATUS" IS '하루종일일정 여부 N기본값 Y종일처리';


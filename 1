COMMENT ON COLUMN "REFERENCE"."REF_MEM_NO" IS '참조자 사원 번호';

CREATE TABLE "APP_ACCEPT" (
	"ACC_NO"	NUMBER		NOT NULL,
	"APP_NO"	NUMBER		NOT NULL,
	"ACC_MEM_NO"	NUMBER		NOT NULL,
	"STATUS"	VARCHAR2(50)	DEFAULT '대기'	NOT NULL,
	"ACC_DATE"	DATE	DEFAULT SYSDATE	NULL
);

COMMENT ON COLUMN "APP_ACCEPT"."ACC_NO" IS 'SEQ_ACCNO';

